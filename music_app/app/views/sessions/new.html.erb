<div class="">
  


<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <h1>Log in</h1>
  <label>Email</label>
  <input type="text" name="users[email]">
  <label>Password</label>
  <input type="password" name="users[password]">
  <br><br>
  <input type="submit" value="Log in">
  
</form>
<br>
<%= button_to "Sign up", new_user_url, method: :get %>

</div>